{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch - SetProperties",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/SetProperties/bin/Debug/net5.0/SetProperties.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/SetProperties",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Launch - Simulator",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/SimulateData/bin/Debug/net5.0/SimulateData.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/SimulateData",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Attach to .NET Functions",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:azureFunctions.pickProcess}"
        }
    ]
}